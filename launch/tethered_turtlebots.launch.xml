<launch>

  <!-- Launch arguments -->
  <arg name="model_name"/>

  <!-- Gazebo model spawner -->
  <arg name="sdf_file" value="$(find tethered_turtlebots_description)/models/tethered_turtlebots.sdf" />
  <node name="spawn_turtlebot_model" pkg="gazebo_ros" type="spawn_model"
	    args="-file $(arg sdf_file) -sdf -model $(arg model_name)"/>

</launch>

