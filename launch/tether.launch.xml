<launch>
  <!-- launcher arguments -->
  <arg name="object_name"/>
  <arg name="init_pose"/>

  <!-- Convert an xacro and put on parameter server -->
  <arg name="urdf_file" default="$(find xacro)/xacro.py '$(find tether_description)/models/tether.urdf.xacro'" />
  <param name="robot_description" command="$(arg urdf_file)" />
  
  <!-- Gazebo model spawner -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"
        args="$(arg init_pose) -unpause -urdf -robot_namespace robot2 -param robot_description -model $(arg object_name)"/>


</launch>
